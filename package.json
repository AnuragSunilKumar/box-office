{
  "homepage": "https://AnuragSunilKumar.github.io/box-office",
  "name": "box-office",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "eslint": "^7.27.0",
    "gh-pages": "^3.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fade-in": "^2.0.1",
    "react-router-dom": "^5.2.0",
